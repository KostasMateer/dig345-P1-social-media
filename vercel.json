{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/index.js",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "npm run build",
          "outputDirectory": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  